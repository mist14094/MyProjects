                    <Items>
                        <asp:MenuItem NavigateUrl="~/Default.aspx" Text="Home"/>
                        <asp:MenuItem NavigateUrl="~/BEPGrid.aspx" Text="Break Even Grid"/>
                        <%--<asp:MenuItem NavigateUrl="~/BEItemDetail.aspx" Text="ItemDetail"/>--%>
                        <asp:MenuItem NavigateUrl="~/About.aspx" Text="About"/>
