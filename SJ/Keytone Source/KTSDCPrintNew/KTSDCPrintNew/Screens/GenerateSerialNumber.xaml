<Window x:Class="KTone.Win.KTSDCPrint.GenerateSerialNumber"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Background="{DynamicResource BackGround}" Style="{DynamicResource MainWindoStyle}"
    Title="PrintSerialNumber" Height="300" Width="450" ResizeMode="NoResize">
    <Grid Width="420" Height="263">
        <GroupBox Header="SerialNumber" BorderBrush="Black" Margin="12,26,21,84" Name="grpSerial">
            <Grid Height="116" >
                <Label Height="28" Margin="14,17,0,0" Name="lblSerial" Width="114" VerticalAlignment="Top" Content="Serial Number" HorizontalAlignment="Left" />
                <TextBox Height="24" BorderBrush="#FF000000" Margin="134.008,17,15.743,0" Name="textBox1" VerticalAlignment="Top" Width="225.249" />
                <Label Height="28" HorizontalAlignment="Left" Margin="14,54,0,34" Name="lblCopies" Width="120" Content="No. Of Copies"/>
                <TextBox Height="24" BorderBrush="#FF000000" Margin="134.008,58,178.992,34" Name="textBox2" Width="62" />
            </Grid>
        </GroupBox>
        <Button Height="25" HorizontalAlignment="Right" Margin="0,0,51,39" Name="btnNext" VerticalAlignment="Bottom" Width="83" Content="Next >>"/>
        <Button Height="25" Margin="164,0,180,39" Name="btnLast" VerticalAlignment="Bottom" Width="76" Content=">> Last"/>
    </Grid>
</Window>
