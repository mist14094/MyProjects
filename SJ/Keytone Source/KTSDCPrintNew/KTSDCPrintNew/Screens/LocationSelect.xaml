<Window x:Class="KTone.Win.KTSDCProvision.LocationSelect"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Style="{DynamicResource MainWindowStye}" Background="{DynamicResource BackGround}"
    Title="LocationSelect" Height="400" Width="300">
    <Grid>        
        <Label x:Name="lblCaption" Content="Select Location" Height="25" Width="108.92" VerticalAlignment="Top" HorizontalAlignment="Left"></Label>
        <Border Margin="4,44,4,4" BorderThickness="2,2,2,2" CornerRadius="4,4,4,4" Style="{DynamicResource MainBorder}">
            <DockPanel LastChildFill="True" x:Name="dckPanel" Margin="5,5,5,18" Height="300" Width="250" />
        </Border>
    </Grid>
</Window>
